#include<iostream>
#include<string>
using namespace std;

template <class NameType,class AgeType>
class Person{
public:
	Person(NameType name,AgeType age){
		this->m_Name=name;
		
		this->m_age=age;
	}
	NameType m_Name;
	AgeType m_age; 
};
void test01(){
	Person<string,int> p1("tom",10);
}
int main(){
 	test01();

	return 0; 
	
} 
